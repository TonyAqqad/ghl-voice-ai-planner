      "requirements": ["business_phone_number", "emergency_phone_number"]
    },
    "webhooks": {
      "base_url": "{{env.WEBHOOK_BASE_URL}}",
      "timeout": 1500,
      "retry_attempts": 3,
      "authentication": "bearer_token"
