  "requirements": {
    "gohighlevel": {
      "version": "2.0+",
      "features": ["custom_fields_v2", "workflows", "opportunities", "calendar_integration"],
      "api_limits": {
        "requests_per_hour": 1000,
        "custom_fields_per_location": 100,
        "workflows_per_location": 50
