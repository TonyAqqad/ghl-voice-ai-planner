    "voice_ai_provider": {
      "provider": "elevenlabs",
      "model": "gpt-4o",
      "voice_id": "21m00Tcm4TlvDq8ikWAM",
      "monthly_minutes": 1000,
      "concurrent_calls": 10
    },
    "phone_system": {
